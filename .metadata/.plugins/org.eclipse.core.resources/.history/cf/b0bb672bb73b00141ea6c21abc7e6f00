package twgitter.croudia;

import twgitter.croudia.config.Config;
import twgitter.get.GetHTTP;

public class GetOAuth {
	public static void main(String[] args) throws Exception {
		String URI = "https://api.croudia.com/oauth/authorize?response_type=code&client_id=" + Config.ConsumerKey + "&state=77f094743bdb55c0f97";
		System.out.println(GetHTTP.AccessHTTPString(URI,"UTF-8"));

	}
}
