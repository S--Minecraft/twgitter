package twgitter.general;

import java.util.List;

public class AllMessages {
	//取得元
	String AppType;


	//Postの実態URL
	String post_url;
	//投稿日時
	String created_at;


	//Postの内容
	String html;
	String text;
	//付加された画像などのメディア
	AllEntity entity;
	//ハッシュタグ・リンク・メンション情報
	List<AllHashtag> hashtag;
	List<AllLink> link;
	List<AllMention> mention;


	//PostのID
	String id;
	String thread_id;//親PostのID


	//リプ数・リポスト数・ファボ数・スプレッド数
	//スプレッド(リポスト&ファボを両方したようなもの)
	int num_replies;
	int num_reposts;
	int num_stars;
	int num_spread;
	//自分がリポスト・ファボ・スプレッドしたか
	boolean you_reposted;
	boolean you_starred;
	boolean you_spreaded;


	//返信先のUserの表示名
	String in_reply_to_screen_name;
	//返信先のUserのID
	int in_reply_to_user_id;
	//返信先のPostのID
	int in_reply_to_status_id;


	//ユーザー情報
	AllUser user;


	//クライアント情報
	ClientInf clientInf;


	//実際に投稿者が投稿したかどうか(APIでないか)
	boolean machine_only;


	//PaginationのID
	int pagination_id;
}
